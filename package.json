{
  "name": "bragging_rights",
  "version": "1.0.0",
  "description": "Site originally built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flash-Git/BraggingRights.git"
  },
  "author": "Flash",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.1.1",
    "express": "^4.18.2",
    "mongoose": "^5.6.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  },
  "bugs": {
    "url": "https://github.com/Flash-Git/BraggingRights/issues"
  },
  "homepage": "https://github.com/Flash-Git/BraggingRights#readme"
}
